<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="modules" type="Prism.Modularity.ModulesConfigurationSection, Prism.Wpf" />
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <appSettings>
    <add key="LoggingTimeInterval" value="1" />
    <add key="LoggingIntervalType" value="Minutes" />
    <add key="AllowedApplicationIdleTime" value="30" />
    <add key="AllowedApplicationIdleIntervalType" value="Minutes" />
    <add key="MqttServerIP" value="127.0.0.1" />
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd" autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">

    <targets>
      <!--<target name="file" xsi:type="File" fileName="${specialfolder:folder=DesktopDirectory}/Resources/Logs/${shortdate}.log" layout="${longdate} ${uppercase:${level}} ${message} ${exception:format=tostring}" />-->
      <target name="file" xsi:type="File" filename="${specialfolder:folder=DesktopDirectory}/Resources/Logs/${cached:cached=true:Inner=${date:format=yyyy-MM-dd hh.mm.ss}:CacheKey=${shortdate}}.log" layout="${longdate} ${uppercase:${level}} ${message} ${exception:format=tostring}" />
    </targets>

    <rules>
      <logger name="*" minlevel="Trace" writeTo="file" />
    </rules>
  </nlog>
