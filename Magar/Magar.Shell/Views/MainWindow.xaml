<Window x:Class="Magar.Shell.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:helpers="clr-namespace:E3.ReactorManager.Interfaces.Helpers;assembly=E3.ReactorManager.Interfaces"
        helpers:UiHelpers.WindowLoadedCommand="{Binding LoadedCommand}"
        WindowStartupLocation="CenterOwner" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="SelectedViewPane" />
    </Grid>
</Window>
