<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="modules" type="Prism.Modularity.ModulesConfigurationSection, Prism.Wpf"/>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <appSettings>
    <add key="LoggingIntervalType" value="Seconds"/>
    <add key="LoggingTimeInterval" value="3"/>
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
     autoReload="true"
     throwExceptions="false"
     internalLogLevel="Off">

    <targets>
      <target name="file" xsi:type="File" fileName="${specialfolder:folder=DesktopDirectory}/Resources/Logs/${shortdate}.log" layout="${longdate} ${uppercase:${level}} ${message} ${exception:format=tostring}"/>
    </targets>

    <rules>
      <logger name="*" minlevel="Trace" writeTo="file" />
    </rules>
  </nlog>
  <modules>
    <module moduleName="SessionManagerModule" startupLoaded="true" assemblyFile="E3.SessionManager.dll" moduleType="E3.SessionManager.SessionManagerModule, E3.SessionManager, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
      <dependencies>
        <dependency moduleName="DataAbstractionLayerModule" />
      </dependencies>
    </module>
    <module assemblyFile="E3.ReactorManager.ReportsManager.dll" moduleType="E3.ReactorManager.ReportsManager.ReportsManagerModule, 
          E3.ReactorManager.ReportsManager, 
          Version=1.0.0.0, 
          Culture=neutral, PublicKeyToken=null" moduleName="ReportsManagerModule" startupLoaded="true">
      <dependencies>
        <dependency moduleName="DataAbstractionLayerModule"/>
      </dependencies>
    </module>
    <module assemblyFile="E3.ReactorManager.Framework.dll" moduleType="E3.ReactorManager.Framework.FrameworkModule, 
        E3.ReactorManager.Framework, 
        Version=1.0.0.0, 
        Culture=neutral, PublicKeyToken=null" moduleName="FrameworkModule" startupLoaded="true">
    </module>
    <module assemblyFile="E3Tech.Navigation.dll" moduleType="E3Tech.Navigation.NavigationModule, 
        E3Tech.Navigation, 
        Version=1.0.0.0, 
        Culture=neutral, PublicKeyToken=null" moduleName="NavigationModule" startupLoaded="true">
      <dependencies>
        <dependency moduleName="FrameworkModule"/>
      </dependencies>
    </module>
    <module assemblyFile="MainMotorPanel.UI.dll" moduleType="MainMotorPanel.UI.UIModule, 
          MainMotorPanel.UI, 
          Version=1.0.0.0, 
          Culture=neutral, PublicKeyToken=null" moduleName="UIModule" startupLoaded="true">
      <dependencies>
        <dependency moduleName="HardwareAbstractionLayerModule"/>
      </dependencies>
    </module>
    <module assemblyFile="E3.ReactorManager.DataAbstractionLayer.dll" moduleType="E3.ReactorManager.DataAbstractionLayer.DataAbstractionLayerModule, 
          E3.ReactorManager.DataAbstractionLayer, 
          Version=1.0.0.0, 
          Culture=neutral, PublicKeyToken=null" moduleName="DataAbstractionLayerModule" startupLoaded="true">
      <dependencies>
        <dependency moduleName="NavigationModule"/>
        <dependency moduleName="FrameworkModule"/>
      </dependencies>
    </module>
    <module assemblyFile="E3.HardwareAbstractionLayer.dll" moduleType="E3.HardwareAbstractionLayer.HardwareAbstractionLayerModule, 
          E3.HardwareAbstractionLayer, 
          Version=1.0.0.0, 
          Culture=neutral, PublicKeyToken=null" moduleName="HardwareAbstractionLayerModule" startupLoaded="true">
      <dependencies>
        <dependency moduleName="DataAbstractionLayerModule"/>
        <dependency moduleName="ControllerProviderModule"/>
      </dependencies>
    </module>
    <module assemblyFile="E3.ReactorManager.ControllerProvider.dll" moduleType="E3.ReactorManager.ControllerProvider.ControllerProviderModule, 
          E3.ReactorManager.ControllerProvider, 
          Version=1.0.0.0, 
          Culture=neutral, PublicKeyToken=null" moduleName="ControllerProviderModule" startupLoaded="true">
    </module>
  </modules>
  <connectionStrings>
    <add name="DBconnection" connectionString="Data Source = .\SQLEXPRESS;Initial Catalog = Satavahana.MainMotorPanel;Integrated Security=true" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>